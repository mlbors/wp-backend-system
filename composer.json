{
  "name": "wp-system",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "A WordPress admin system built with Sage and ACF Pro.",
  "homepage": "https://github.com/mlbors/wp-system/",
  "authors": [
    {
      "name": "Mátyás Lancelot Bors",
      "homepage": "https://github.com/mlbors"
    }
  ],
  "keywords": [
